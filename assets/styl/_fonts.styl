@import url('https://fonts.googleapis.com/css?family=Montserrat:300,400,500');

$base   = -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif

// $fonts = 'SongStd', 'FranklinGothic-Medium'

@font-face {
	font-family: 'SongStd';
	src: url('../fonts/SongStd.eot');
	src: url('../fonts/SongStd.eot?#iefix') format('embedded-opentype'),
		url('../fonts/SongStd.woff') format('woff'),
		url('../fonts/SongStd.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'FranklinGothic-Medium';
	src: url('../fonts/FranklinGothic-Medium.eot');
	src: url('../fonts/FranklinGothic-Medium.eot?#iefix') format('embedded-opentype'),
		url('../fonts/FranklinGothic-Medium.woff') format('woff'),
		url('../fonts/FranklinGothic-Medium.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

